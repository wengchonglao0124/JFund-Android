<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f6f6f6"
    tools:context=".Activity.SignUpActivity">

    <Button
        android:id="@+id/id_btn_signUp_back"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/chevron_back1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="70dp"
        android:fontFamily="@font/dm_sans"
        android:text="Sign up"
        android:textColor="#120d26"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view9"
        android:layout_width="321dp"
        android:layout_height="46dp"
        android:layout_marginTop="124dp"
        android:background="@drawable/rounded_rectangle_gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/user_icon"
        app:layout_constraintBottom_toBottomOf="@+id/view9"
        app:layout_constraintStart_toStartOf="@+id/view9"
        app:layout_constraintTop_toTopOf="@+id/view9" />

    <EditText
        android:id="@+id/id_et_signUp_name"
        android:layout_width="270dp"
        android:layout_height="20dp"
        android:layout_marginStart="11dp"
        android:background="@null"
        android:fontFamily="@font/dm_sans"
        android:hint="Username"
        android:lineSpacingExtra="5.6sp"
        android:singleLine="true"
        android:textColor="#1e1e1e"
        android:textColorHint="#b9b9bd"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/view9"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/view9" />

    <View
        android:id="@+id/view10"
        android:layout_width="321dp"
        android:layout_height="46dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/rounded_rectangle_gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view9" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/mail"
        app:layout_constraintBottom_toBottomOf="@+id/view10"
        app:layout_constraintStart_toStartOf="@+id/view10"
        app:layout_constraintTop_toTopOf="@+id/view10" />

    <EditText
        android:id="@+id/id_et_signUp_email"
        android:layout_width="270dp"
        android:layout_height="20dp"
        android:layout_marginStart="11dp"
        android:background="@null"
        android:fontFamily="@font/dm_sans"
        android:hint="Email"
        android:lineSpacingExtra="5.6sp"
        android:singleLine="true"
        android:textColor="#1e1e1e"
        android:textColorHint="#b9b9bd"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/view10"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/view10" />

    <View
        android:id="@+id/view11"
        android:layout_width="321dp"
        android:layout_height="46dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/rounded_rectangle_gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view10" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/base_lock_22"
        app:layout_constraintBottom_toBottomOf="@+id/view11"
        app:layout_constraintStart_toStartOf="@+id/view11"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <EditText
        android:id="@+id/id_et_signUp_password1"
        android:layout_width="230dp"
        android:layout_height="20dp"
        android:layout_marginStart="11dp"
        android:background="@null"
        android:fontFamily="@font/dm_sans"
        android:hint="Password"
        android:lineSpacingExtra="5.6sp"
        android:singleLine="true"
        android:textColor="#1e1e1e"
        android:textColorHint="#b9b9bd"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/view11"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <View
        android:id="@+id/view12"
        android:layout_width="321dp"
        android:layout_height="46dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/rounded_rectangle_gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view11" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/base_lock_22"
        app:layout_constraintBottom_toBottomOf="@+id/view12"
        app:layout_constraintStart_toStartOf="@+id/view12"
        app:layout_constraintTop_toTopOf="@+id/view12" />

    <EditText
        android:id="@+id/id_et_signUp_password2"
        android:layout_width="230dp"
        android:layout_height="20dp"
        android:layout_marginStart="11dp"
        android:background="@null"
        android:fontFamily="@font/dm_sans"
        android:hint="Confirm Password"
        android:lineSpacingExtra="5.6sp"
        android:singleLine="true"
        android:textColor="#1e1e1e"
        android:textColorHint="#b9b9bd"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/view12"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toTopOf="@+id/view12" />

    <Button
        android:id="@+id/id_btn_signUp"
        android:layout_width="321dp"
        android:layout_height="44dp"
        android:layout_marginTop="414dp"
        android:background="@drawable/sign_up_button2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView23"
        app:layout_constraintEnd_toStartOf="@+id/textView23"
        app:layout_constraintTop_toTopOf="@+id/textView23" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="67dp"
        android:layout_marginTop="489dp"
        android:layout_marginEnd="97dp"
        android:fontFamily="@font/dm_sans"
        android:lineSpacingExtra="5.6sp"
        android:text="You accept JFundâ€™s                           "
        android:textColor="#1e1e20"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dm_sans"
        android:lineSpacingExtra="5.6sp"
        android:text="Terms of use"
        android:textColor="#57287e"
        android:textSize="14sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/textView23"
        app:layout_constraintEnd_toEndOf="@+id/textView23"
        app:layout_constraintTop_toTopOf="@+id/textView23" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="71dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="38dp"
        android:fontFamily="@font/dm_sans"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="10sp"
        android:text="Already have an account?               "
        android:textColor="#120d26"
        android:textSize="15sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/id_tv_signIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/dm_sans"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="10sp"
        android:text="Sign in"
        android:textColor="#57287e"
        android:textSize="15sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="@+id/textView25"
        app:layout_constraintEnd_toEndOf="@+id/textView25"
        app:layout_constraintTop_toTopOf="@+id/textView25" />

    <Button
        android:id="@+id/id_btn_signUp_eye1"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_marginEnd="11dp"
        android:background="@drawable/sign_in_eye"
        app:layout_constraintBottom_toBottomOf="@+id/view11"
        app:layout_constraintEnd_toEndOf="@+id/view11"
        app:layout_constraintTop_toTopOf="@+id/view11" />

    <Button
        android:id="@+id/id_btn_signUp_eye2"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_marginEnd="11dp"
        android:background="@drawable/sign_in_eye"
        app:layout_constraintBottom_toBottomOf="@+id/view12"
        app:layout_constraintEnd_toEndOf="@+id/view12"
        app:layout_constraintTop_toTopOf="@+id/view12" />

</androidx.constraintlayout.widget.ConstraintLayout>